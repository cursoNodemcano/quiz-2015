<div>
	<table width="100%">
		<% for (var i=0;i < quizes.length; i++) { %>
			<tr>
				<td width="50%"><a href="quizes/<%= quizes[i].id %>"><%= quizes[i].pregunta %></a></td>
				<td width="50%"><a href="quizes/<%= quizes[i].id %>/edit"><button>Editar</button></a></td>
			</tr>
		<% } %>
	</table>
	<br/>
	<a href="/quizes/new"><button type="button">Crear pregunta</button></a>
</div>
<form method="get" action="/quizes">
	<p>Buscar preguntas que contengan:</p>
	<input type="text" name="search" />
	<input type="submit" value="Buscar" />
</form>
